{
  "projectType": "library",
  "sourceRoot": "libs/client-shared/ui/flight-card/src",
  "prefix": "flight-card",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/client-shared/ui/flight-card"],
      "options": {
        "jestConfig": "libs/client-shared/ui/flight-card/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/client-shared/ui/flight-card/**/*.ts",
          "libs/client-shared/ui/flight-card/**/*.html"
        ]
      }
    },
    "component-test": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "libs/client-shared/ui/flight-card/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "flight-app:build"
      }
    }
  },
  "tags": ["scope:client-shared", "type:ui"]
}
